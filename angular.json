{
  "version": 1,
  "cli": {
    "analytics": false,
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "RentCar",
  "projects": {
    "RentCar": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "less"
        }
      },
      "root": "apps/RentCar",
      "sourceRoot": "apps/RentCar/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/RentCar",
            "index": "apps/RentCar/src/index.html",
            "main": "apps/RentCar/src/main.ts",
            "polyfills": "apps/RentCar/src/polyfills.ts",
            "tsConfig": "apps/RentCar/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/RentCar/src/favicon.ico",
              "apps/RentCar/src/assets"
            ],
            "styles": [
              "apps/RentCar/src/styles.less",
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeng/resources/primeng.min.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/RentCar/src/environments/environment.ts",
                  "with": "apps/RentCar/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "RentCar:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "RentCar:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "RentCar:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/RentCar/src/test.ts",
            "polyfills": "apps/RentCar/src/polyfills.ts",
            "tsConfig": "apps/RentCar/tsconfig.spec.json",
            "karmaConfig": "apps/RentCar/karma.conf.js",
            "assets": [
              "apps/RentCar/src/favicon.ico",
              "apps/RentCar/src/assets"
            ],
            "styles": ["apps/RentCar/src/styles.less"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/RentCar/tsconfig.app.json",
              "apps/RentCar/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "RentCar:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "RentCar:serve:production"
            }
          }
        }
      }
    },
    "RentCar-e2e": {
      "root": "apps/RentCar-e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/RentCar-e2e/protractor.conf.js",
            "devServerTarget": "RentCar:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "RentCar:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/RentCar-e2e/tsconfig.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "api-rent-car": {
      "root": "apps/api-rent-car",
      "sourceRoot": "apps\\api-rent-car\\src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api-rent-car",
            "main": "apps/api-rent-car/src/main.ts",
            "tsConfig": "apps/api-rent-car/tsconfig.app.json",
            "assets": ["apps/api-rent-car/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api-rent-car/src/environments/environment.ts",
                  "with": "apps/api-rent-car/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api-rent-car:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api-rent-car/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api-rent-car"],
          "options": {
            "jestConfig": "apps/api-rent-car/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
